{
  "name": "contributte/vite",
  "description": "Vite integration for Nette Framework.",
  "keywords": [
    "nette",
    "vite"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/vite",
  "authors": [
    {
      "name": "Lubomír Blažek",
      "email": "email@lubomirblazek.cz",
      "homepage": "https://lubomirblazek.cz"
    },
    {
      "name": "Milan Matějček",
      "email": "milan.matejcek@gmail.com"
    }
  ],
  "support": {
    "email": "email@lubomirblazek.cz",
    "issues": "https://github.com/contributte/vite/issues"
  },
  "require": {
    "php": ">=8.2",
    "nette/application": "^3.0.8",
    "nette/di": "^3.1.8",
    "tracy/tracy": "^2.10.5"
  },
  "require-dev": {
    "contributte/phpstan": "^0.2.0",
    "contributte/qa": "^0.4.0",
    "contributte/tester": "^0.3.0",
    "latte/latte": "^3.0.12"
  },
  "suggest": {
    "tracy/tracy": "to enable asset debugging in the debug bar panel",
    "latte/latte": "to enable Latte integration via {vite} macro"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\Vite\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.3.x-dev"
    }
  }
}
